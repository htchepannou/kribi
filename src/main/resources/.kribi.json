{
  "name": "kribi",
  "template": "springboot",
  "runAs": "kribi-ec2-role",
  "healthCheckPath": "/health",
  "jvmOptions": "-Xmx1500M -Xms1500Mb",
  "instances": [
    {
      "region": "us-east-1",
      "type": "t2.micro"
    }
  ],
  "services": {
    "http": true,
    "httpPort": 8080,
    "https": false,
    "httpsPort": 8442,
    "ssh": true,
    "sshPort": 22
  }
}
